<?xml version='1.0' encoding='utf-8'?>
<widget id="com.brick.app_v1" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SQLitePlugin">
        <param name="android-package" value="io.liteglue.SQLitePlugin" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="com.cordova.plugin.localNotification.LocalNotification" />
    </feature>
    <name>Brick-mobile</name>
    <description>
        Calculadora de materiales para android.
    </description>
    <author email="jhellmetal2000@gmail.com" href="https://github.com/jsmorales/brick_cordova_android">
      Ingeniero de Desarrollo Johan Morales
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
